#include <stdio.h>
#include <stdlib.h>

int IngresarEntero(int numero);

int main(void) {
	setbuf(stdout,NULL);
	int ingreseEntero;

	printf("Ingrese un numero: ");
	scanf("%d",&ingreseEntero);

	IngresarEntero(ingreseEntero);



	printf("El numero ingresado es: %d\n", ingreseEntero);
	return EXIT_SUCCESS;
}

